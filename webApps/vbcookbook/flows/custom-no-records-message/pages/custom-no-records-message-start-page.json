{
  "title": "Custom No Records Message Recipe",
  "description": "",
  "variables": {
    "employeeListSDP": {
      "type": "vb/ServiceDataProvider",
      "defaultValue": {
        "endpoint": "businessObjects/getall_Employee",
        "keyAttributes": "id",
        "itemsPath": "items",
        "responseType": "page:getallEmployeeResponse"
      }
    }
  },
  "metadata": {},
  "types": {
    "getallEmployeeResponse": {
      "items": [
        {
          "firstName": "string",
          "id": "number",
          "lastName": "string"
        }
      ]
    },
    "@decorators.getallEmployeeResponse": {
      "endpoint": "getall_Employee",
      "service": "businessObjects",
      "path": "response"
    }
  },
  "chains": {},
  "eventListeners": {
    "filterButtonOjAction": {
      "chains": [
        {
          "chainId": "FilterActionChain",
          "parameters": {
            "detail": "{{ $event.detail }}"
          }
        }
      ]
    },
    "ojButtonOjAction": {
      "chains": [
        {
          "chainId": "onRefreshQuery",
          "parameters": {
            "key": "{{ $current.key }}",
            "index": "{{ $current.index }}",
            "current": "{{ $current }}"
          }
        }
      ]
    }
  },
  "imports": {
    "components": {
      "oj-table": {
        "path": "ojs/ojtable"
      },
      "oj-button": {
        "path": "ojs/ojbutton"
      }
    }
  }
}