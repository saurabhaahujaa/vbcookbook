{
  "title": "Triggers Recipe",
  "description": "",
  "variables": {
    "employeeListSDP": {
      "type": "vb/ServiceDataProvider",
      "defaultValue": {
        "endpoint": "businessObjects/getall_Employee",
        "keyAttributes": "id",
        "itemsPath": "items",
        "responseType": "page:getallEmployeeResponse"
      }
    },
    "oj_table_11161435_1SelectedId": {
      "type": "string",
      "defaultValue": ""
    }
  },
  "metadata": {},
  "types": {
    "@decorators.getallEmployeeResponse": {
      "endpoint": "getall_Employee",
      "service": "businessObjects",
      "path": "response"
    },
    "getallEmployeeResponse": {
      "items": [
        {
          "id": "number",
          "job": "number",
          "jobObject": {
            "items": [
              {
                "jobTitle": "string"
              }
            ]
          },
          "lastName": "string",
          "manager": "number",
          "managerObject": {
            "items": [
              {
                "lastName": "string"
              }
            ]
          },
          "salary": "number"
        }
      ]
    }
  },
  "chains": {},
  "eventListeners": {
    "editEmployeeButtonClicked": {
      "chains": [
        {
          "chainId": "navigateToEditEmployeeChain",
          "parameters": {
            "employeeId": "[[ $page.variables.oj_table_11161435_1SelectedId ]]"
          }
        }
      ]
    },
    "oj_table_11161435_1SelectionChanged": {
      "chains": [
        {
          "chainId": "oj_table_11161435_1ChangeSelectionChain",
          "parameters": {
            "employeeId": "[[ $event.detail.value.key || '' ]]"
          }
        }
      ]
    }
  },
  "imports": {
    "components": {
      "oj-list-view": {
        "path": "ojs/ojlistview"
      },
      "oj-label": {
        "path": "ojs/ojlabel"
      },
      "oj-table": {
        "path": "ojs/ojtable"
      },
      "oj-toolbar": {
        "path": "ojs/ojtoolbar"
      },
      "oj-button": {
        "path": "ojs/ojbutton"
      }
    }
  }
}