{
  "title": "Dashboard Template Recipe",
  "description": "",
  "variables": {
    "actionsBarVisible": {
      "type": "boolean",
      "defaultValue": false
    },
    "chartData": {
      "type": "vb/ArrayDataProvider2",
      "defaultValue": {
        "data": "{{ $variables.chartDataArray }}",
        "itemType": "ChartType",
        "keyAttributes": "id"
      }
    },
    "chartDataArray": {
      "type": "ChartType[]",
      "defaultValue": [
        {
          "id": 1,
          "series": "01 - Qualification (10)",
          "group": "Group 1",
          "value": 10
        },
        {
          "id": 2,
          "series": "02 - Discovery (15)",
          "group": "Group 1",
          "value": 15
        },
        {
          "id": 3,
          "series": "03 - Building Vision (25)",
          "group": "Group 1",
          "value": 25
        },
        {
          "id": 4,
          "series": "04 - Presentation (21)",
          "group": "Group 1",
          "value": 21
        },
        {
          "id": 5,
          "series": "05 - Agreement (17)",
          "group": "Group 1",
          "value": 17
        },
        {
          "id": 6,
          "series": "06 - Negotiation (27)",
          "group": "Group 1",
          "value": 27
        }
      ]
    },
    "notificationData": {
      "type": "vb/ArrayDataProvider2",
      "defaultValue": {
        "data": "{{ $variables.notificationDataArray }}",
        "itemType": "NotificationType",
        "keyAttributes": "id"
      }
    },
    "notificationDataArray": {
      "type": "NotificationType[]",
      "defaultValue": [
        {
          "id": 1,
          "name": "New Messages",
          "value": 10,
          "badge": 1
        },
        {
          "id": 1,
          "name": "New Reports",
          "value": 3,
          "badge": 1
        },
        {
          "id": 1,
          "name": "Overdue Tasks",
          "value": 12,
          "badge": 2
        },
        {
          "id": 1,
          "name": "Today Meetings",
          "value": 4,
          "badge": 3
        }
      ]
    }
  },
  "metadata": {},
  "types": {
    "ChartType": {
      "group": "string",
      "id": "number",
      "series": "string",
      "value": "number"
    },
    "NotificationType": {
      "id": "number",
      "name": "string",
      "value": "number",
      "badge": "number"
    }
  },
  "eventListeners": {
    "buttonAction": {
      "chains": [
        {
          "chainId": "ToggleActionsBarChain"
        }
      ]
    },
    "vbEnter": {
      "chains": [
        {
          "chainId": "ToggleActionsBarChain",
          "parameters": {}
        },
        {
          "chainId": "FadeInMainContent",
          "parameters": {}
        }
      ]
    }
  },
  "imports": {
    "components": {
      "oj-avatar": {
        "path": "ojs/ojavatar"
      },
      "oj-button": {
        "path": "ojs/ojbutton"
      },
      "oj-chart": {
        "path": "ojs/ojchart"
      },
      "oj-chart-item": {
        "path": "ojs/ojchart"
      },
      "oj-list-item-layout": {
        "path": "ojs/ojlistitemlayout"
      },
      "oj-list-view": {
        "path": "ojs/ojlistview"
      },
      "oj-option": {
        "path": "ojs/ojoption"
      },
      "oj-radioset": {
        "path": "ojs/ojradioset"
      }
    },
    "css": [
      "/resources/css/dashboard-template.css"
    ]
  }
}