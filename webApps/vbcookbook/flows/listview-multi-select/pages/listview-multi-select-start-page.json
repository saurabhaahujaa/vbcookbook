{
  "title": "Multi Select Recipe",
  "description": "",
  "variables": {
    "employeeListSDP": {
      "type": "vb/ServiceDataProvider",
      "defaultValue": {
        "endpoint": "businessObjects/getall_Employee",
        "keyAttributes": "id",
        "itemsPath": "items",
        "responseType": "page:getallEmployeeResponse"
      }
    },
    "selectedEmployeeIds": {
      "type": "number[]"
    }
  },
  "metadata": {},
  "types": {
    "@decorators.getallEmployeeResponse": {
      "endpoint": "getall_Employee",
      "service": "businessObjects",
      "path": "response"
    },
    "getallEmployeeResponse": {
      "items": [
        {
          "email": "string",
          "firstName": "string",
          "id": "number",
          "lastName": "string",
          "phoneNumber": "string",
          "salary": "number"
        }
      ]
    }
  },
  "chains": {},
  "eventListeners": {
    "listviewMultiselectSelected": {
      "chains": [
        {
          "chainId": "ListViewSelectedChangeChain",
          "parameters": {
            "keys": "{{Array.from($event.detail.value.values())}}"
          }
        }
      ]
    }
  },
  "imports": {
    "components": {
      "oj-label": {
        "path": "ojs/ojlabel"
      },
      "oj-list-view": {
        "path": "ojs/ojlistview"
      },
      "oj-avatar": {
        "path": "ojs/ojavatar"
      },
      "oj-selector": {
        "path": "ojs/ojselector"
      }
    }
  }
}